---
import { Image } from 'astro:assets';
import Confetti1 from '../images/confetti/confetti1.svg';
import Confetti6 from '../images/confetti/confetti6.svg';
import Confetti7 from '../images/confetti/confetti7.svg';
import Confett8 from '../images/confetti/confetti8.svg';
import Confetti10 from '../images/confetti/confetti10.svg';
import Confetti12 from '../images/confetti/confetti12.svg';
import Confetti14 from '../images/confetti/confetti14.svg';
---

<Image
	src={Confetti1}
	alt='confetti1'
	class='absolute top-0 left-[60px] customXs:left-[120px] z-10'
/>
<Image
	src={Confetti6}
	alt='confetti6'
	class='absolute top-[-10px] right-[200px] z-10'
/>
<Image
	src={Confetti7}
	alt='confetti7'
	class='absolute top-[-10px] right-[50px] z-10'
/>
<Image
	src={Confett8}
	alt='confetti8'
	class='absolute top-[60px] -left-[100px] z-10'
/>
<Image
	src={Confetti10}
	alt='confetti10'
	class='absolute top-[150px] left-[-10px] z-10'
/>
<Image
	src={Confetti1}
	alt='confetti1'
	class='absolute top-[15px] left-[45%] z-10'
/>
<Image
	src={Confetti12}
	alt='confetti12'
	class='absolute top-[10px] right-[10px] z-10'
/>
<Image
	src={Confetti14}
	alt='confetti14'
	class='absolute top-[220px] left-[300px] z-10'
/>
