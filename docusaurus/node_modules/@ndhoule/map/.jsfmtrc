{
  "lineBreak" : {
    "before" : {
      "EndOfFile" : 1,
      "ReturnStatement": 1,
      "TryKeyword": 1
    }
  },
  "whiteSpace": {
    "before": {
      "BlockComment": 0
    },
    "after": {
      "BlockComment": 0
    }
  },
  "quotes": {
    "type": "single",
    "avoidEscape": false
  },
  "plugins": [
    "esformatter-braces",
    "esformatter-quotes",
    "esformatter-semicolons"
  ]
}
