---
import { Image } from 'astro:assets';
import Confetti1 from '../images/confetti/confetti1.svg';
import Confetti6 from '../images/confetti/confetti6.svg';
import Confetti7 from '../images/confetti/confetti7.svg';
import Confett8 from '../images/confetti/confetti8.svg';
import Confetti2 from '../images/confetti/confetti2.svg';
import Confetti5 from '../images/confetti/confetti5.svg';
import Confetti9 from '../images/confetti/confetti9.svg';
import Confetti10 from '../images/confetti/confetti10.svg';
import Confetti11 from '../images/confetti/confetti11.svg';
import Confetti12 from '../images/confetti/confetti12.svg';
import Confetti13 from '../images/confetti/confetti13.svg';
import Confetti14 from '../images/confetti/confetti14.svg';
---

<Image
	src={Confetti1}
	alt='confetti1'
	class='absolute top-0 left-[120px] -z-1'
/>
<Image
	src={Confetti6}
	alt='confetti6'
	class='invisible customMd:visible absolute top-[-100px] right-[200px] -z-1'
/>
<Image
	src={Confetti7}
	alt='confetti7'
	class='absolute top-[-50px] right-[50px] -z-1'
/>
<Image
	src={Confetti5}
	alt='confetti5'
	class='absolute top-[-240px] right-[250px] -z-1'
/>
<Image
	src={Confetti9}
	alt='confetti9'
	class='absolute top-[-200px] right-[400px] -z-1'
/>
<Image
	src={Confetti11}
	alt='confetti11'
	class='invisible customMd:visible absolute top-[20px] right-[70px] -z-1'
/>
<Image
	src={Confetti12}
	alt='confetti12'
	class='absolute top-[10px] right-[10px] -z-1'
/>
<Image
	src={Confetti6}
	alt='confetti6'
	class='absolute -top-[170px] right-[500px] blur-sm -z-1'
/>
<Image
	src={Confetti13}
	alt='confetti13'
	class='absolute -top-[100px] right-[600px] -z-1'
/>
<Image
	src={Confetti14}
	alt='confetti14'
	class='absolute top-[300px] left-[320px] -z-1'
/>
