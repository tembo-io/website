---
import { Image } from 'astro:assets';
import Confetti1 from '../images/confetti/confetti1.svg';
import Confetti6 from '../images/confetti/confetti6.svg';
import Confetti3 from '../images/confetti/confetti3.svg';
import Confetti8 from '../images/confetti/confetti8.svg';
import Confetti2 from '../images/confetti/confetti2.svg';
import Confetti5 from '../images/confetti/confetti5.svg';
import Confetti9 from '../images/confetti/confetti9.svg';
import Confetti10 from '../images/confetti/confetti10.svg';
import Confetti11 from '../images/confetti/confetti11.svg';
import Confetti12 from '../images/confetti/confetti12.svg';
import Confetti13 from '../images/confetti/confetti13.svg';
import Confetti15 from '../images/confetti/confetti15.svg';
---

<Image
	src={Confetti1}
	alt='confetti1'
	class='absolute top-0 left-[120px] -z-1'
/>
<Image
	src={Confetti6}
	alt='confetti6'
	class='absolute top-0 left-[320px] min-[780px]:block hidden'
/>
<Image
	src={Confetti10}
	alt='confetti10'
	class='absolute top-[220px] -right-[80px] -z-1'
/>
<Image src={Confetti8} alt='confetti8' class='absolute top-0 right-0' />
<Image
	src={Confetti5}
	alt='confetti5'
	class='absolute min-[780px]:top-[200px] left-0 top-[100px]'
/>
<Image
	src={Confetti10}
	alt='confetti10'
	class='absolute top-[400px] left-0 md:block hidden'
/>
<Image
	src={Confetti12}
	alt='confetti12'
	class='min-[1400px]:block absolute top-[200px] left-[600px] hidden'
/>
<Image
	src={Confetti3}
	alt='confetti12'
	class='absolute top-[100px] left-[150px] min-[550px]:block hidden'
/>
<Image
	src={Confetti15}
	alt='confetti12'
	class='absolute top-[200px] right-[100px] min-[1150px]:block hidden'
/>
